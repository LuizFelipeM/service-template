{
	"name": "service-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "tsx watch --inspect=0.0.0.0:9229 ./src/app.ts",
		"build": "pnpm '/^build:.*/'",
		"build:server": "tsc",
		"build:client": "vite build --ssr src/presentation/pages/entry-server.tsx --ssrManifest --outDir dist/server",
		"lint": "eslint",
		"check:src": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,json}' --config ./.prettierrc",
		"check:all": "prettier --check . --config ./.prettierrc",
		"format:src": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' --config ./.prettierrc",
		"format:all": "prettier --write . --config ./.prettierrc"
	},
	"packageManager": "pnpm@9.14.2",
	"dependencies": {
		"amqp-simple-client": "^2.0.0",
		"dotenv": "^16.4.5",
		"express": "^5.0.1",
		"pg": "^8.13.1",
		"react-dom": "^18.3.1",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.8.0",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.0",
		"@types/node": "^22.7.7",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@vitejs/plugin-react-swc": "^3.7.2",
		"compression": "^1.7.5",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.37.2",
		"globals": "^15.13.0",
		"prettier": "3.4.1",
		"react": "^18.3.1",
		"sirv": "^3.0.0",
		"tsx": "^4.19.2",
		"typescript": "~5.6.3",
		"typescript-eslint": "^8.16.0",
		"vite": "^6.0.0"
	}
}